page{
  height:100%;
}
.all_page{
  width: 100%;
  height: 100%;
}
.background {
  width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%;
}

.btnbox{
  display: flex;
  justify-content: center;
  
}
.startbtn{
  width: 174px;
  height: 62px;
  background-size: 100% 100%;
  position: fixed;
  margin-top: 145%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.title{
  position: absolute;
  top: 10%;
  right: 10%;
}

 
.height45{
  height: 42%;
}
.center{
  display: flex;
  align-content: center;
  align-items: center;
  justify-content: center;
}

.con{
  width : 100%;
  height : 100%;  
  position: fixed;  
  z-index: 0;  
}
.row{
  position: relative;
}
.center_img{
  position: absolute;
  bottom: 0;
}

.flex-row{
  display: flex;
  flex-direction: row;
}



/* 文化 */
.round-word{
  border: 1px solid #9b4b4b;
  border-radius: 500rpx;
  width: 100rpx;
  height: 100rpx;
  font-size: 13pt;
}
.margin-top25{
  margin-top: 25%;
}
.margin-left5{
  margin-left: 5%;
}
.margin-top2{
  margin-top: 5%;
}
/*有一种 */
.left_text{
  width: 10pt;
  font-size: 10pt;
  color: #505050;
}
.right-text{
  display: flex;
  flex-direction: column;
  color: #505050;
}
.down_text{
  width: 12pt;
  font-size: 12pt;
  color: #505050;
}
.down-box{
  width: 40%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-content: center;
  align-items: center;
}
.btn_box{
  position: relative;
}
.btn_bg{
  width : 100%;
  height : 100%;  
  position: fixed;  
  z-index: -1;   
}
